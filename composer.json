{
    "name": "j45l/functional-php",
    "description": "Functional primitives for PHP",
    "keywords": [
        "functional"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jordi Bisbal",
            "email": "jordi.bisbal+functional@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1|~8",
        "lstrojny/functional-php": "^1.17"
    },
    "require-dev": {
        "slevomat/coding-standard": "^7.0",
        "phpstan/phpstan": "^1.4",
        "phpmd/phpmd": "^2.11",
        "roave/security-advisories": "dev-latest",
        "squizlabs/php_codesniffer": "~3.0",
        "friendsofphp/php-cs-fixer": "~3.0",
        "jetbrains/phpstorm-attributes": "^1.0",
        "infection/infection": "^0.23.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "j45l\\functional\\": "src"
        },
        "files": [
            "src/Best.php",
            "src/CartesianProduct.php",
            "src/CrossCompareSet.php",
            "src/Delay.php",
            "src/Fold.php",
            "src/FoldRight.php",
            "src/Merge.php",
            "src/Pluck.php",
            "src/Reduce.php",
            "src/ReduceRight.php",
            "src/RepeatPipe.php",
            "src/Take.php",
            "src/ToArray.php",
            "src/ToIterable.php",
            "src/TryOrThrow.php",
            "src/Unindex.php",
            "src/Worst.php",
            "src/YieldIterable.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "j45l\\functional\\Test\\": "tests"
        }
    },
    "scripts": {
        "tests": "vendor/bin/phpunit",
        "coding-style": "vendor/bin/phpcs && vendor/bin/php-cs-fixer fix --dry-run --diff --config=.php_cs.dist",
        "clear": "rm -rf vendor/"
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
